<% if(result != null) {
             if(result.code == 1 && result.obj != null){
                 var score = result.obj;%>
                <br>
                <table class="student">
                    <tr>
                        <td class="student_name">学校:</td>
                        <td class="student_value"><%= score.school %></td>
                        <td class="student_name">班级:</td>
                        <td class="student_value"><%= score.grade + "(" + score.classId + ")" %></td>
                    </tr>
                    <tr>
                        <td class="student_name">考号:</td>
                        <td class="student_value"><%= score.no %></td>
                        <td class="student_name">姓名:</td>
