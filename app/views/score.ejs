                    </tr> 
                    </tr>  
                        <tr>  
                        <th scope="row" abbr="G5 Processor" class="specalt">物理</th>  
                        <td class="alt"><%= score.physicsScore %></td>  
                        <td class="alt"><%= score.physicsClassOrder %></td>  
                        <td class="alt"><%= score.physicsGradeOrder %></td>  
                    </tr> 
                    </tr>  
                        <tr>  
                        <th scope="row" abbr="G5 Processor" class="specalt">生物</th>  
                        <td class="alt"><%= score.biologyScore %></td>  
                        <td class="alt"><%= score.biologyClassOrder %></td>  
                        <td class="alt"><%= score.biologyGradeOrder %></td>  
                    </tr> 
                    </tr>  
                    <tr>  
                        <th scope="row" abbr="G5 Processor" class="specalt">化学</th>  
                        <td class="alt"><%= score.chemistryScore %></td>  
                        <td class="alt"><%= score.chemistryClassOrder %></td>  
                        <td class="alt"><%= score.chemistryGradeOrder %></td>  
                    </tr>         
                </table> 
               
             <% }else if(result.message != null && result.message.length  > 0 ){
                   console.log(result + "##############gggg################3");
                 %>

                    <p><p class="ERROR_MEAASGE"><b><%= result.message%></b></p></p>
            <% }
         }%>  